{
  "eslint.workingDirectories": ["./functions"],
  "cSpell.words": [
    "firebaseapp",
    "firebasestorage",
    "firestore",
    "geofire",
    "geohash",
    "nodenext",
    "pawdar",
    "predeploy",
    "webp"
  ],
